{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "customTemplates": {
        "component": "templates/components/default/TemplateName.tsx",
        "lazy": "templates/components/default/TemplateName.lazy.tsx"
      },
      "withLazy": true,
      "withStory": false,
      "withStyle": true,
      "withTest": true
    },
    "storyComponent": {
      "path": "src/components",
      "withLazy": true,
      "withStory": true,
      "withStyle": true,
      "withTest": true
    },
    "page": {
      "path": "src/pages",
      "customTemplates": {
        "component": "templates/components/page/TemplateName.tsx",
        "lazy": "templates/components/page/TemplateName.lazy.tsx"
      },
      "withLazy": true,
      "withStory": false,
      "withStyle": true,
      "withTest": true
    }
  }
}
